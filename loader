			if v.Name ~= player.Name then
				TOTAL = TOTAL + 1
			end
		end
	end
	if TOTAL > 0 then
		announce('Found','A total of ' .. tostring(TOTAL) .. ' Purge X Users', 3)
	else
		announce('D:', 'No Purge X Users Found', 3)
	end
end)





